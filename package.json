{
  "name": "tauri-version",
  "type": "module",
  "version": "0.4.1",
  "private": false,
  "packageManager": "pnpm@9.11.0",
  "description": "Update version for tauri-app.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3xysteak/tauri-version.git"
  },
  "keywords": [
    "tauri",
    "release",
    "releases",
    "cli"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./cli.mjs",
  "files": [
    "*.mjs",
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "tsc && unbuild",
    "build:only": "unbuild",
    "test": "vitest",
    "test:once": "vitest run && pnpm e2e",
    "e2e": "esno ./scripts/e2e.ts",
    "release": "tsc && pnpm test:once && bumpp",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.7.4",
    "bumpp": "^9.6.1",
    "eslint": "^9.12.0",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.2"
  }
}
