{
  "name": "tauri-version",
  "type": "module",
  "version": "1.0.0-beta.1",
  "private": false,
  "packageManager": "pnpm@9.14.2",
  "description": "Update version for tauri-app.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3xysteak/tauri-version.git"
  },
  "keywords": [
    "tauri",
    "release",
    "releases",
    "cli"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./legend": {
      "types": "./dist/legend/index.d.ts",
      "import": "./dist/legend/index.mjs",
      "require": "./dist/legend/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/cli.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "tsc && unbuild",
    "build:only": "unbuild",
    "test": "vitest",
    "test:once": "vitest run && pnpm e2e",
    "e2e": "esno ./scripts/e2e.ts",
    "e2e:legend": "esno ./scripts/e2e-legend.ts",
    "release": "bumpp",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "consola": "^3.2.3",
    "pathe": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.0",
    "bumpp": "catalog:",
    "eslint": "^9.15.0",
    "smol-toml": "^1.3.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.5"
  }
}
