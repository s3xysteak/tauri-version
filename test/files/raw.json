{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "lethal-company-mods-copier",
    "version": "0.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "os": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": ["**/*"]
      },
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "powershell",
            "cmd": "powershell.exe",
            "args": true
          }
        ]
      }
    },
    "windows": [
      {
        "title": "lethal-company-mods-copier",
        "width": 400,
        "height": 300
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "s3xysteak-lethal-company-mods-copier",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
